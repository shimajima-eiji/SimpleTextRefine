{
    "name": "simple-text-refine-with-gpt",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.84.1"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "simple-text-refine-with-gpt.helloWorld",
                "title": "Simple Text Refine w/ GPT: say hello world"
            },
            {
                "command": "simple-text-refine-with-gpt.callGPT",
                "title": "Simple Text Refine w/ GPT: call GPT"
            },
            {
                "command": "simple-text-refine-with-gpt.openPrompt",
                "title": "Simple Text Refine w/ GPT: open prompt file"
            }
        ],
        "languages": [
            {
                "id": "yaml",
                "extensions": [
                    ".prompt"
                ],
                "aliases": ["YAML", "yaml"],
                "filenames": []
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Simple Text Refine w/ GPT",
            "properties": {
                "simple-text-refine-with-gpt.api_key": {
                    "type": "string",
                    "default": "",
                    "description": "OpenAI API key"
                }
            }
        }
    },
    "dependencies": {
        "openai": "^4.23.0"
    }
}
